<!--IMC控制卡配置-->
<CONFIG>
	<AXISCONFIG>
		<AXIS>
			<NAME>X</NAME>					<!--轴名称-->
			<NUMB>1</NUMB>					<!--轴编号-->
			<PPR>10000</PPR>				<!--每转脉冲数-->
			<SCREW>10</SCREW>				<!--丝杆导程-->
			<ACCELIM>20000000</ACCELIM>		<!--最大加速度限制-->
			<VELLIM>50000000</VELLIM>		<!--最大速度-->
			<MCSMAXVEL>200000000</MCSMAXVEL><!--点到点运动速度-->
			<MCSACCEL>100000</MCSACCEL>		<!--主坐标系加速度-->
			<MCSDECEL>100000</MCSDECEL>		<!--主坐标系减速度-->
			<SMOOTH>16</SMOOTH>				<!--点到点运动平滑因子-->
			<EXITFILT>0</EXITFILT>			<!--发生错误时，轴是否退出运行-->
			<STOPFILT>1</STOPFILT>			<!--发生错误时，轴是否停止运行-->
			<STEPMOD>6</STEPMOD>			<!--脉冲发生模式-->
			<STEPTIM>50</STEPTIM>			<!--一般伺服20以上-->
			<AIOCTRL>1418</AIOCTRL>			<!--轴IO设置-->
			<SETTLEW>1</SETTLEW>			<!--静止误差补偿阈值-->
